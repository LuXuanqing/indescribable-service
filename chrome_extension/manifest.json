{
    "manifest_version": 2,
    "name": "previews for javlib",
    "version": "0.2.0",
    "description": "显示影片预览图",
    "content_scripts": [{
            "matches": ["http://www.javlibrary.com/cn/?v=*"],
            "js": ["vue.js", "javlib.js"]
        },
        {
            "matches": ["https://www.javbus.com/*-*"],
            "js": ["javbus.js"]
        }
    ]
}